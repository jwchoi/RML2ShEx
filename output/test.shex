PREFIX ex: <http://example.com/>
PREFIX my: <http://my.example/ns#>
PREFIX ql: <http://semweb.mmlab.be/ns/ql#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
BASE <http://my.example/ns#>

my:SA0 my:NC2 IRI /^http:\/\/example\.com\/sport\/(.*)$/ AND CLOSED {
  $my:TC1 ex:id my:NC3 LITERAL ?;
  $my:TC2 ex:description my:NC4 LITERAL ?
}

my:SA1 my:NC0 IRI /^http:\/\/example\.com\/student\/(.*)$/ AND CLOSED {
  $my:TC0 ex:plays my:NC1 IRI /^http:\/\/example\.com\/sport\/(.*)$/ ?
}

my:SA2 my:NC5 IRI /^http:\/\/example\.com\/student\/(.*)$/ AND CLOSED {
  $my:TC3 ex:firstName my:NC6 LITERAL ?;
  $my:TC4 ex:lastName my:NC7 LITERAL ?
}

my:SA3 @my:SA2 AND @my:SA1

my:SO0 @my:SA1 OR @my:SA3

my:SO1 @my:SA2 OR @my:SA3

