PREFIX ex: <http://example.com/>
PREFIX my: <http://my.example/ns#>
PREFIX ql: <http://semweb.mmlab.be/ns/ql#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
BASE <http://my.example/ns#>

my:SA0 IRI /^http:\/\/example\.com\/sport\/(.*)$/ AND CLOSED {
  ex:description LITERAL ?;
  ex:id LITERAL ?
}

my:SA1 IRI /^http:\/\/example\.com\/student\/(.*)$/ AND CLOSED {
  ex:plays IRI /^http:\/\/example\.com\/sport\/(.*)$/ ?
}

my:SA2 IRI /^http:\/\/example\.com\/student\/(.*)$/ AND CLOSED {
  ex:firstName LITERAL ?;
  ex:lastName LITERAL ?
}

my:SA3 @my:SA1 AND @my:SA2

my:SO0 @my:SA1 OR @my:SA3

my:SO1 @my:SA2 OR @my:SA3

