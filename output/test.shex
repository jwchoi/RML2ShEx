PREFIX ex: <http://example.com/>
PREFIX dept: <http://example.com/dept#>
PREFIX my: <http://my.example/ns#>
PREFIX ql: <http://semweb.mmlab.be/ns/ql#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
BASE <http://my.example/ns#>

my:SA0 IRI /^http:\/\/example\.com\/resource\/sport_(.*)$/ {
  rdfs:label LITERAL ?
}

my:SA10 @my:SA4 AND @my:SA5 AND @my:SA6

my:SA1 IRI /^http:\/\/data\.example\.com\/employee\/(.*)$/ {
  ex:role IRI /^http:\/\/data\.example\.com\/roles\/(.*)$/ ?
}

my:SA2 IRI /^http:\/\/example\.com\/resource\/student_(.*)$/ {
  <http://example.com/ontology/practises> @my:SO1 ?;
  foaf:name LITERAL ?
}

my:SA3 BNODE {
  dept:COMPANY LITERAL ?;
  dept:deptno xsd:positiveInteger ?;
  dept:location LITERAL ?;
  dept:name LITERAL ?
}

my:SA4 IRI /^http:\/\/example\.com\/(.*)$/ {
  a [ex:Site ex:Student] {2};
  foaf:name LITERAL ?
}

my:SA5 IRI /^http:\/\/example\.com\/(.*)$/ {
  rdfs:label [@es] ?
}

my:SA6 IRI /^http:\/\/example\.com\/(.*)$/ {
  ^<http://example.com/ontology/practises> @my:SA2 ?;
  rdfs:label [@en] ?
}

my:SA7 @my:SA4 AND @my:SA5

my:SA8 @my:SA4 AND @my:SA6

my:SA9 @my:SA5 AND @my:SA6

my:SO0 @my:SA10 OR @my:SA5 OR @my:SA7 OR @my:SA9

my:SO1 @my:SA10 OR @my:SA6 OR @my:SA8 OR @my:SA9

my:SO2 @my:SA10 OR @my:SA4 OR @my:SA7 OR @my:SA8

