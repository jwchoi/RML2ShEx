PREFIX ex: <http://example.com/>
PREFIX my: <http://my.example/ns#>
PREFIX ql: <http://semweb.mmlab.be/ns/ql#>
PREFIX rml: <http://semweb.mmlab.be/ns/rml#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
BASE <http://my.example/ns#>

my:S0 IRI /^http:\/\/example\.com\/sport\/(.*)$/ AND CLOSED {
  ex:description LITERAL ?;
  ex:id LITERAL ?
}

my:S1 @my:S3 OR @my:S5

my:S2 @my:S4 OR @my:S5

my:S3 IRI /^http:\/\/example\.com\/student\/(.*)$/ AND CLOSED {
  $my:T0 ex:plays IRI /^http:\/\/example\.com\/sport\/(.*)$/ ?
}

my:S4 IRI /^http:\/\/example\.com\/student\/(.*)$/ AND CLOSED {
  $my:T1 (
    ex:firstName LITERAL ?;
    ex:lastName LITERAL ?
  )
}

my:S5 IRI /^http:\/\/example\.com\/student\/(.*)$/ AND CLOSED {
  &my:T0;
  &my:T1
}

